{
	"name": "gravatar-mcp-server-remote",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"gravatar-mcp-server": "dist/index.js"
	},
	"scripts": {
		"dev": "nodemon --exec tsx src/index.ts",
		"start": "node dist/index.js",
		"build": "tsc && chmod +x dist/index.js",
		"format": "biome format --write",
		"lint": "biome lint",
		"lint:fix": "biome lint --fix",
		"type-check": "tsc --noEmit",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"generate-kubb": "npx kubb generate",
		"generate-kubb-with-spec": "make download-spec && npx kubb generate"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "1.15.1",
		"express": "^4.19.2",
		"cors": "^2.8.5",
		"zod": "^3.25.67"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.6",
		"@types/express": "^4.17.21",
		"@types/cors": "^2.8.17",
		"@types/node": "^24.0.10",
		"@kubb/core": "^3.15.0",
		"@kubb/plugin-client": "^3.15.0",
		"@kubb/plugin-oas": "^3.15.0",
		"@kubb/plugin-ts": "^3.15.0",
		"@kubb/plugin-zod": "^3.15.0",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"msw": "^2.10.3",
		"nodemon": "^3.0.2",
		"tsx": "^4.0.0",
		"typescript": "^5.8.3",
		"universal-user-agent": "^7.0.0",
		"vitest": "^3.2.4"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
